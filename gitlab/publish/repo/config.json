{
  "dockerImageBuilder": "cimg/base:stable-20.04",
  "inputs": [
    {
      "name": "username",
      "type": "CREDENTIAL_GITLAB_USERNAME"
    },
    {
      "name": "token",
      "type": "CREDENTIAL_GITLAB_TOKEN"
    },
    {
      "name": "email",
      "type": "CREDENTIAL_GITLAB_EMAIL"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value. ",
        "qty": 6
      },
      "label": "Project Name : ",
      "name": "project_name",
      "type": "text",
      "tutorial": "Your project name e.g.: awesome-project"
    },
    {
      "items": [
        "private",
        "public",
        "internal"
      ],
      "label": "What will be the visibility of the repository? ",
      "name": "privacy",
      "type": "text",
      "tutorial": "Choose one option"
    },
    {
      "label": "Workspace to publish path:",
      "name": "workspace_path",
      "type": "text",
      "tutorial": "Workspace path i.e., /home/user/dennis/ritchie-formulas"
    },
    {
      "default": "v1.0.0",
      "label": "Version: (default: v1.0.0)",
      "name": "version",
      "type": "text",
      "tutorial": "The version of your project i.e., v1.0.0"
    }
  ]
}
