{
  "dockerImageBuilder": "cimg/go:1.14",
  "inputs": [
    {
      "name" : "test_duration",
      "type" : "text",
      "label" : "Test duration in seconds",
      "cache" : {
        "active": true,
        "qtd" : 5,
        "newLabel" : "Previous test duration value"
      }
    },
    {
      "name" : "max_threads",
      "type" : "text",
      "label" : "Number of threads to be used in the test",
      "cache" : {
        "active": true,
        "qtd" : 5,
        "newLabel" : "Previous max threads value"
      }
    }
  ]
}
