{
  "dockerImageBuilder": "cimg/go:1.14",
  "inputs": [
    {
      "name": "access_key",
      "type": "CREDENTIAL_AWS_ACCESSKEYID"
    },
    {
      "name": "secret_access_key",
      "type": "CREDENTIAL_AWS_SECRETACCESSKEY"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value. ",
        "qty": 6
      },
      "label": "Type your region: ",
      "name": "region",
      "type": "text",
      "tutorial": "AWS region i.e., us-east-1, us-west-1, sa-east-1"
    },
    {
      "label": "Type bucket name: ",
      "name": "bucket",
      "type": "text",
      "tutorial": "Bucket name e.g.: some-aws-bucket"
    }
  ]
}
