{
  "dockerImageBuilder": "cimg/base:stable-20.04",
  "inputs": [
    {
      "name": "access_key",
      "type": "CREDENTIAL_AWS_ACCESSKEYID"
    },
    {
      "name": "secret_access_key",
      "type": "CREDENTIAL_AWS_SECRETACCESSKEY"
    },
    {
      "default": "us-east-1",
      "items": [
        "sa-east-1",
        "us-east-2",
        "us-east-1",
        "us-west-1",
        "us-west-2",
        "af-south-1",
        "ap-east-1",
        "ap-south-1",
        "ap-northeast-3",
        "ap-northeast-2",
        "ap-southeast-1",
        "ap-southeast-2",
        "ap-northeast-1",
        "ca-central-1",
        "cn-north-1",
        "cn-northwest-1",
        "eu-central-1",
        "eu-west-1",
        "eu-west-2",
        "eu-south-1",
        "eu-west-3",
        "eu-north-1",
        "me-south-1"
      ],
      "label": "Pick the region: ",
      "name": "region",
      "tutorial": "Choose one option",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value: ",
        "qty": 6
      },
      "label": "Cluster Name: ",
      "name": "cluster_name",
      "tutorial": "Your EKS cluster name",
      "type": "text"
    }
  ]
}
