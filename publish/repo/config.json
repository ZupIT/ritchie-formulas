{
  "dockerImageBuilder": "cimg/base:stable-20.04",
  "inputs": [
    {
      "items": [
        "Github",
        "Gitlab"
      ],
      "label": "Provider : ",
      "name": "provider",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new value. ",
        "qty": 6
      },
      "label": "Project Name : ",
      "name": "project_name",
      "type": "text"
    },
    {
      "default": "true",
      "items": [
        "true",
        "false"
      ],
      "label": "Will it be a private repository? ",
      "name": "privacy",
      "type": "text"
    },
    {
      "label": "Workspace to publish path: (ex: /home/user/dennis/ritchie-formulas)",
      "name": "workspace_path",
      "type": "text"
    },
    {
      "default": "v1.0.0",
      "label": "Version: (default: v1.0.0)",
      "name": "version",
      "type": "text"
    }
  ]
}
