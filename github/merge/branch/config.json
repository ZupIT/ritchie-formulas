{
  "dockerImageBuilder": "cimg/go:1.14",
  "inputs": [
    {
      "cache": {
        "active": true,
        "newLabel": "Type new branch. ",
        "qty": 100
      },
      "default": "qa",
      "label": "Select destination branch(default qa):",
      "name": "branch",
