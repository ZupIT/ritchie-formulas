{
  "dockerImageBuilder": "cimg/go:1.14",
  "inputs": [
    {
      "default": "maven-project",
      "items": [
        "maven-project",
        "gradle-project"
      ],
      "label": "Select type project: ",
      "name": "type",
      "type": "text",
      "tutorial": "Choose one option"
    },
    {
      "default": "kotlin",
      "items": [
        "java",
        "kotlin",
        "groovy"
      ],
      "label": "Select language project: ",
      "name": "language",
      "type": "text",
      "tutorial": "Choose one option"
    },
    {
      "default": "2.2.4.RELEASE",
      "items": [
        "2.3.0.M2",
        "2.3.0.BUILD-SNAPSHOT",
        "2.2.5.BUILD-SNAPSHOT",
        "2.2.4.RELEASE",
        "2.1.13.BUILD-SNAPSHOT",
        "2.1.12.RELEASE"
      ],
      "label": "Select Spring Boot version: ",
      "name": "boot_version",
      "type": "text",
      "tutorial": "Choose one option"
    },
    {
      "label": "Type your groupId: ",
      "name": "group_id",
      "type": "text",
      "tutorial": "Project group ID i.e., br.com.zup"
    },
    {
      "label": "Type your artifactId: ",
      "name": "artifact_id",
      "type": "text",
      "tutorial": "The name of your jar file without the version i.e., demo"
    },
    {
      "label": "Type the project`s description: ",
      "name": "description",
      "type": "text",
      "tutorial": "A description for your project i.e., project demo"
    },
    {
      "default": "jar",
      "items": [
        "war",
        "jar"
      ],
      "label": "Pick your packaging type: ",
      "name": "packaging",
      "type": "text",
      "tutorial": "Choose one option"
    },
    {
      "default": "11",
      "items": [
        "13",
        "11",
        "1.8"
      ],
      "label": "Pick your java version: ",
      "name": "java_version",
      "type": "text",
      "tutorial": "Choose a java version"
    },
    {
      "label": "Enter the dependencies ids: ",
      "name": "dependencies",
      "type": "text",
      "tutorial": "type witch dependencies you want preload i.e., web,postgresql. For list dependencies: 'rit scaffold show spring-starter-dependencies' "
    }
  ]
}
